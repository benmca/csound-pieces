(in-package :stella)(defparameter *totaldur* 0);(defobject quick (csound-note)  ((instr :initform "i") inst dur amp)  (:parameters instr inst time dur amp));;(defobject revnote (csound-note);  ((name :initform "i99") mystart dur revtime);  (:parameters name mystart dur revtime));(defun bassnare (st len)  (algorithm nil quick (start st length len)	(in-tempo 320) 	(setf inst(item (items 1 2 in random)))	(setf rhythm (item (rhythms s e q 12 12 12  in rewrite				rules '( (s (e) -> s s e q)					)			tempo (tempo 140))))	(setf dur rhythm)	(setf amp 1)	));;(defun hat (st len)  (algorithm nil quick (start st length len)	(in-tempo 320) 	(setf inst 3)	(setf rhythm (item (rhythms q tempo (tempo 140))))	(setf dur rhythm)	(setf amp 1)));;(defun mrn (&key (revtime 10.));  (algorithm nil revnote (start 10000 length 1 revtime revtime rhythm 0);	     (setf mystart 0);	     (setf dur (+ revtime *totaldur* 1));	     ));(defun ms (&key (start-time 1.)(my-scorefile "c:/Ben/dnb3.sco"))  (let* ((myheader (header "f 1 0 16384 9 .5 1 0"			   )))    (fheader my-scorefile myheader)    (merge all ()	    (bassnare 1 200)		(hat 1 80)            )    (open-cmd my-scorefile)	(mix-cmd "all")))