sr=44010kr=4401ksmps=10nchnls=2instr 1idur=p3iamp=p4*10ipitch=p5ipartial=p6inum=int(p6)+1;simple envelope for attack;decent cheap polish texture if many notes are present around one pitchkenv	linen		iamp, idur*.2, idur, idur*.5avib	oscili	10, 2, 1asig	buzz		kenv, ipitch, inum, 1amod	oscili	ipartial*(ipitch*2), ipitch*2, 1acar	oscili	kenv*.5, ipitch+amod, 1asig2 oscili	kenv*1.1, ipitch+avib, 1outs asig+asig2+acar, asig+asig2+acarendininstr 2idur=p3iamp=p4ipitch=p5ictrindx=p6kenv	oscil1i		0, iamp, idur, 2knois	randh		3, 20	;randh!!!!kenv2	linseg		1.5, p3, 1; insert kenv2 in pitch of carrier=gradual slide to pitch if 1->1.01kpit	oscil1i		0, 1,idur*2, 1; in kpit, 	if idur>real idur, slower half sine shape do sound,;		if <real idur, quicker...amod	oscili	(ictrindx+cpsoct(knois))*(ipitch*2), ipitch*2, 1acar	oscili	kenv, (kpit*(ipitch+amod)), 1	outs		acar, acarendininstr 3idur=p3iamp=p4ipitch1=p5*1.16	;ratio of 5:7 (fc:fm) for bellipitch2=p5ipitch3=p5*.6875	;ratio of 16:11 (fc:fm) for drumimax=p6kenv	oscil1i	0, iamp, idur, 2	;attack envkdx	oscil1i	0, imax, idur, 2	;index env, tied to attackaosc1	oscili	iamp, 1/idur, 2aosc2	oscili	(ipitch1)*imax, 1/idur, 2aosc3 	oscili	aosc2, ipitch1, 1aosc4	oscili	aosc1, ipitch2+aosc3, 1amod1	oscili	kdx*ipitch1, ipitch1, 1a1	oscili	kenv, ipitch2+amod1, 1amod2	oscili	kdx*ipitch1, ipitch1, 1a2	oscili	kenv*.7, (ipitch2*2)+amod2, 1amod3	oscili	kdx*ipitch1, ipitch1, 1a3	oscili	kenv*.6, (ipitch2*3)+amod3, 1outs aosc4, a1+a2+a3endin