sr=22050kr=2205ksmps=10nchnls=1instr 1;; gnawa sample at 22050 has wide attack (all bins (atck)); and stable material (asus), centered around 60, 120, 240, etc to 840.;katcktm	linseg	0, .0348, .0348, p3-.0348, 0  ksustm	linseg	0, .0348, .0348, p3-.0348, .331atckamp	linseg	1, .0348, 1, .01, 0, p3-.0348, 0asusamp	linseg	0, .0348, 0, .01, 1, p3-.0348, 1kenva	linen	150, .01, p3, .01kenvs	linen	200, .01, p3, .01atck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asus	pvadd	ksustm, 1, "gnawa.pvoc", 1, 100, 1;	out	(kenva*(atck*atckamp))	out	(kenva*(atck*atckamp));+(kenvs*(asus*asusamp));	out	atckampendininstr 2;; miroslav upright sample (harmonic); katcktm	linseg	0, .01, .01, p3-.01, 0  ksustm	linseg	0, .01, .01, p3-.01, 1.005atckamp	linseg	1, .01, 1, .01, 0, p3-.01, 0asusamp	linseg	0, .01, 0, .01, 1, p3-.01, 1kenva	linen	150, .01, p3, .01kenvs	linen	200, .01, p3, .01atck	pvadd	katcktm, 1, "miro.pvoc", 1, 1024,1asus	pvadd	ksustm, 1, "miro.pvoc", 1, 400, 1	out	(kenva*(atck*atckamp));+(kenvs*(asus*asusamp))	endininstr 3ipitch=p5/120iatck=.077iamp = p4index=iamp   ;*.5ihalfpi = 3.1413 * .5itwopi = 3.143 * 2katcktm	linseg	0, iatck+.5, iatck+.5; ampskenva	linseg	.75, iatck, .75, .01, 0, .5, 0; rampkenvb	linseg 	0,.01, 1, iatck-.02, 1, .01, 0, .5, 0atck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = kenvb*(atck*kenva);;prog control;timout iatck+.1, p3-(iatck+.1), susif katcktm > iatck kgoto susasig2=0kgoto restsus:asig1=0ksustm	linseg	iatck, p3-iatck, .3;kenvs	linen	1, .01, p3-iatck, .01kenvs	linseg	0, .01, 1, p3-iatck, 1kenvsb	linen	1, .01, .1+(p3-iatck), .01asus	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 7, 2asig2 = kenvsb*((asus+asus2+asus3)*kenvs)rest:asigall = (asig1+asig2) / 32767amsg = sin((asigall * ihalfpi) + index * sin(itwopi * asigall))	out	amsg*iampallendininstr 4;;instr 3 with sndwarp;katcktm	linseg	0, .0348, .0348 kenva	linen	150*p4, .01, .0348, .01atck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1timout	.0348, p3-.0348, susasig1 = atck*kenvakgoto restsus:asig1=0ksustm	linseg	.0348, p3-.0348, .331kenvs	linen	100*p4, .01, p3-.0348, .01ipitch = p5/120asus	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 1000, 2asig2 = asus*kenvsrest:	out	asig1+asig2endin