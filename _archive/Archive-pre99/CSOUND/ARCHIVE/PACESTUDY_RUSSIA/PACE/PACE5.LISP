(in-package :stella)(defobject i1 (csound-note)	((instr :initform "i1") dur amp pitch partial)	(:parameters instr time dur amp pitch partial))(defobject i2 (csound-note)	(instr dur amp pitch index)	(:parameters instr time dur amp pitch index))(defun a (start-time len)	(algorithm nil x (start start-time length len)		(vars (half (* len .5)))		(setf rhythm (item (rhythms s q e s s q ts tq te in random tempo 1000)))		(setf dur (* rhythm 3))		(setf amp (interpl count 0 0 half 500 len 0))		(setf pitch (between 220 240))		(setf partial (interpl count 0 0.0 len 4.0))		))(defun gong (start-time len)	(algorithm nil i2 (start start-time length len)		(setf rhythm 6)		(setf dur rhythm)		(setf amp 5000)		(setf pitch (item (items [55 110 165 220])))		(setf instr (item (items [2 3])))		(setf partial (between 1 3))			))(defun ms (&key (start-time 1.)(my-scorefile "pace5.sco"))	(let* ((myheader (header "f 1 0 16384 10 1"					"f2 0 16385 5 1 16384 .0001"					"f3 0 8193 5 1 8192 .0001")))	(fheader my-scorefile myheader)	(merge all ()		(gong 1 1)			)	(open-cmd my-scorefile)	(mix-cmd "all")))