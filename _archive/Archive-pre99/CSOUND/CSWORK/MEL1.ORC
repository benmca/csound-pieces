sr=44100kr=4410ksmps=10nchnls=2ga1 init	0ga2	init	0;;good cheap organ;instr 1iamp=p4idur=p3iamp=p4ipitch = p5ipan = p6		; *idist = p7		; * locsig argsipct = p8		; *iratio = 2imodfreq = p5 * iratioindex = 2krmp	linen	iamp, .01, idur, .01klin	expseg	.1, p3, 1kdev = imodfreq * (index * klin)amod	oscili	kdev, imodfreq, 1acar1	oscili	krmp, p5+amod, 1;acar2	oscili	krmp*.7, 2*(p5+amod), 1;acar3	oscili	krmp*.4, 4*(p5+amod), 1asnd = acar1;+acar2+acar3; locsig statementsar, al		locsig		asnd, ipan, idist, ipctar1, ar2	locsend			outs	ar, al; output to reverbga1=ga1+ar1ga2=ga2+ar2endin;;;	2;;;instr 2iamp=p4idur=p3iamp=p4ipitch = p5ipan = p6		; *idist = p7		; * locsig argsipct = p8		; *iratio = 2imodfreq = p5 * iratioindex = 1idev = imodfreq * indexkrmp	linen	iamp, .01, idur, .01amod	oscili	idev, imodfreq, 1acar1	oscili	krmp, p5+amod, 1;acar2	oscili	krmp*.7, 2*(p5+amod), 1;acar3	oscili	krmp*.4, 4*(p5+amod), 1klin	line	1, p3, p5*7acar1	tone	acar1, klinasnd = acar1; locsig statementsar, al		locsig		asnd, ipan, idist, ipctar1, ar2	locsend			outs	ar, al; output to reverbga1=ga1+ar1ga2=ga2+ar2endininstr 99a1	reverb2	ga1, p4, .5a2	reverb2	ga2, p4, .5	outs	a1,a2ga1=0ga2=0endin