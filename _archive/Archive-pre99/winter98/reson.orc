sr=22050kr=2205ksmps=10nchnls=1instr 1 ;Simple instrument using buzz as input to reson filter.        ;Bandwidth of filter is changed over time using expseg.iamp=p4ifreq=p5iharms=(sr*.4) / ifreq  ;Number of Harmonics for buzz to produce.kbw     expseg  10000, p3*.2, 10000, p3*.7, 1, p3*.1, 1abuzz   buzz    iamp, ifreq, iharms, 1ares    reson    abuzz, ifreq, kbw, 1kenv    linen   1, .05, p3, .05out ares*kenvendininstr 2 ;Simple instrument using rand as input to reson filter.        ;Bandwidth of filter is changed over time using expseg.iamp=p4*.5ifreq=p5kbw     expseg  10000, p3*.2, 10000, p3*.7, 1, p3*.1, 1anoise  rand      iampares    reson    anoise, ifreq, kbw, 2kenv    linen   1, .05, p3, .05out ares*kenvendininstr 3iamp=p4*.5ifreq=p5kcf    expseg   ifreq*4, p3*.2, ifreq*5, p3*.7, ifreq, p3*.1, ifreq*2anoise rand     iampares   reson    anoise, kcf, 50,2kenv    linen   1, .05, p3, .05out ares*kenvendin