(in-package :stella)(defobject x (csound-note)	((instr :initform "i1") dur amp pitch)	(:parameters instr time dur amp pitch))(defun a (start-time len)	(algorithm nil x (start start-time length len)		(vars (half (* len .5)))		(setf rhythm (item (rhythms s q e s s q ts tq te in random tempo 1000)))		(setf dur (* rhythm 2))		(setf amp (interpl count 0 0 half 500 len 0))		(setf pitch (between 220 230))		))(defun ms (&key (start-time 1.)(my-scorefile "pace1.sco"))	(let* ((myheader (header "f 1 0 16384 10 1")))	(fheader my-scorefile myheader)	(merge all ()		(a 1 500)			)	(open-cmd my-scorefile)	(mix-cmd "all")))