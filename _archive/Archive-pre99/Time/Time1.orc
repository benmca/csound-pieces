sr=44100kr=4410ksmps=10nchnls=2instr 1idur=p3iamp=p4ipitch=p5aenv		linen	1, idur*.1, idur, idur*.1a1		soundin	"medium.aif"asig		reson	a1, ipitch, .1asig10		reson	a1, ipitch, 10asig100		reson	a1, ipitch, 100asigf		=	asig+(asig10*(iamp*.1))+(asig100*(iamp*.01))asig		balance	asigf, a1asig1, asig2	locsig	asig, 45, 10, .5			outs	asig1*aenv, asig2	endininstr 99a1	reverb	ga1, p4, .5a2	reverb	ga2, p4, .5	outs	a1,a2ga1=0ga2=0endin