sr=22050kr=2205ksmps=10nchnls=2ga1 	init	0ga2	init	0instr 1idur=p3iamp=p4ipitch=p5kenv		linen	1, idur*.1, idur, idur*.1a1		soundin	"medium.aif"asign		reson	a1, ipitch, .5asig10		reson	a1, ipitch, 10asig100		reson	a1, ipitch, 100asign		balance	asign, a1asig10		balance	asig10, a1asig100		balance	asig100, a1asign2		=	asign*kenv		;						;asig1, asig2	locsig	asign2, 80, 1, .01	; send narrow filter to reverbar1, ar2	locsend				;;asigf		=	(asign+(asig10)+(asig100))*kenv		;;asig3, asig4	locsig	asigf, 20, 1, 1	; pan combined filters, don't send to 'verb;ar3, ar4	locsend;		outs	asig3, asig4ga1=ga1+ar1	ga2=ga2+ar2	endininstr 99a1	reverb	ga1, p4, .5a2	reverb	ga2, p4, .5	outs	a1,a2ga1=0ga2=0endin