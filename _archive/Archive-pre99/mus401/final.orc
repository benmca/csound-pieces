sr=22050kr=2205ksmps=10nchnls=1;instr 1;p7 is index at end of spreadaenv	linen	p4, .01, p3, .01aline	expseg	p5, p3*.75, p6, p3*.25, p6arati	expseg	2, p3*.75, 1, p3*.1, 1, p3*.15, p7amod	oscili	aline*arati, aline*arati, 1acar1	oscili	aenv, aline+amod, 1		out 	acar1endin;instr 2ampenv	expseg	.0001, p3*.1, 1, p3*.1, 1, p3*.1, .6, p3*.9, .0001amodf1 = p4*1.6amodf2 = p4*3.3amod1	oscili	amodf1*3, amodf1, 1amod2	oscili	amodf2*3, amodf2, 1asig = amod1 + amod2acar1	oscili	ampenv*p5, asig+p4, 1	out acar1+amod1+amod2endin;instr 3aenv	linen	p5, p3*.3, p3, p3*.3iformant = int(((p6/p4) +.5) * p4)imodfreq = p4*p8amod	oscili	imodfreq*p7, imodfreq, 1acar1	oscili	aenv*.7, amod+p4, 1acar2	oscili	aenv, amod+iformant, 1asig = acar1+acar2		out asigendin;instr 4aenv	linen	p4, p3*.3, p3, p3*.3aosc1	oscili	aenv, p5, 2		out	aosc1endin