001, 1000*p4, idur-.002, 1000*p4, .001, 0, .1, 0 ; rampatck1, atck2	sndwarp	1, katcktm, 1, 2, 0, 2205, 441, 15, 3, 1atck	balance	atck1, atck2asig1 = atck*kenvtimout idur+.1, p3-idur, zero2if katcktm >= iatck kgoto endsusasig2=0kgoto restzero2:asig1=0kgoto endsusendsus:ksustm	linseg	iatck, p3-idur, ilengthkenvs	linen	3000*p4, .01, p3-idur, p3*.2kpitch	expseg	ipitch, p8+.01, ipitch, (p3-idur)-(p8+((p3-idur)*.61)), ipitch2, (p3-idur)*.6, ipitch2 asus1, asus2	sndwarp	1, ksustm, kpitch, 2, 0, 2205, 441, 15, 3, 1asus	balance	asus1, asus2asig2 = asus*kenvskgoto restallatck:asig2=0katcktm	linseg	0, p3, iatckkenva	linen	1000*p4, .005, p3, .005; rampatck1, atck2	sndwarp	1, katcktm, 1, 2, 0, 2205, 441, 15, 3, 1atck	balance	atck1, atck2asig1 = atck*kenvakgoto restonenote:katcktm	linseg	0, iatck+.1, iatck+.1kenv	linseg 	0,.001, 1000*p4, iatck-.002, 1000*p4, .001, 0, .1, 0 ; rampatck1, atck2	sndwarp	1, katcktm, 1, 2, 0, 2205, 441, 15, 3, 1atck	balance	atck1, atck2asig1 = atck*kenvtimout iatck+.1, p3-iatck, zero4if katcktm >= iatck kgoto onesusasig2=0kgoto restzero4:asig1=0kgoto onesusonesus:ksustm	linseg	iatck, p3-iatck, ilengthkenvs	linen	3000*p4, .01, p3-iatck, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-iatck)-(p8+((p3-iatck)*.61)), ipitch2, (p3-iatck)*.6, ipitch2 asus1, asus2	sndwarp	1, ksustm, kpitch, 2, 0, 2205, 441, 15, 3,1asus	balance	asus1, asus2asig2 = asus*kenvskgoto restrest:	outs	asig1+asig2, asig1+asig2endininstr 3ilength=.3ipitch=p5/120 ;120 is pitch of orig sampleiatck=.07iglis=p6;assign beginning and end pitches for glissandoipitch=p5/120ipitch2=p9/120;type: where the note's at in the phrase; 0=beg, 1=mid, 2=end, 3=nosus, 4=solo; these would be better in separate instrumentstype:if p7=0 kgoto begif p7=1 kgoto midif p7=2 kgoto endif p7=3 kgoto allatckif p7=4 kgoto onenote; change envelopes ONLY to a-ratebeg:katcktm	linseg	0, iatck+.1, iatck+.1kenv	linseg 	0,.01, 10*p4, iatck-.02, 10*p4, .01, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1	asig1 = atck*kenvtimout 	iatck+.1, p3-iatck, zero0if katcktm >= iatck kgoto begsus	asig2=0	kgoto restzero0:asig1=0kgoto begsusbegsus:asig1=0ksustm	linseg	iatck, p3-iatck, ilength ;fade starts .1 before end???kenvs	linen	15*p4, .01, p3-iatck, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-iatck)-(p8+((p3-iatck)*.61)), ipitch2, (p3-iatck)*.6, ipitch2 asus	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 7, 2asig2 = (asus+asus2+asus3)*kenvsasig3 = 1*kpitchkgoto restmid:ibeg=iatck*p10idur=iatck-ibegkatcktm	linseg	ibeg, idur+.1, iatck+.1kenv	linseg 	0,.01, 10*p4, idur-.02, 10*p4, .01, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = atck*kenvtimout idur+.1, p3-idur, zero1if katcktm >= iatck kgoto midsusasig2=0kgoto restzero1:asig1=0kgoto midsusmidsus:ksustm	linseg	iatck, p3-idur, ilength kenvs	linen	15*p4, .01, p3-idur, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-idur)-(p8+((p3-idur)*.61)), ipitch2, (p3-idur)*.6, ipitch2asus	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 7, 2asig2 = (asus+asus2+asus3)*kenvskgoto restend:ibeg=iatck*p10idur=iatck-ibegkatcktm	linseg	ibeg, idur+.1, iatck+.1kenv	linseg 	0,.01, 10*p4, idur-.02, 10*p4, .01, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = atck*kenvtimout idur+.1, p3-idur, zero2if katcktm >= iatck kgoto endsusasig2=0kgoto restzero2:asig1=0kgoto endsusendsus:ksustm	linseg	iatck, p3-idur, ilengthkenvs	linen	15*p4, .01, p3-idur, p3*.1kpitch	expseg	ipitch, p8+.01, ipitch, (p3-idur)-(p8+((p3-idur)*.61)), ipitch2, (p3-idur)*.6, ipitch2asus	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 7, 2asig2 = (asus+asus2+asus3)*kenvskgoto restallatck:asig2=0katcktm	linseg	0, p3, iatck kenva	linen	10*p4, .005, p3, .005; rampatck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = atck*kenvakgoto restonenote:katcktm	linseg	0, iatck+.1, iatck+.1kenv	linseg 	0,.01, 10*p4, iatck-.02, 10*p4, .01, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = atck*kenvtimout iatck+.1, p3-iatck+.1, zero4if katcktm >= iatck kgoto onesus					asig2=0kgoto restzero4:asig1=0kgoto onesusonesus:ksustm	linseg	iatck, p3-iatck, ilengthkenvs	linen	10*p4, .01, p3-iatck, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-iatck)-(p8+((p3-iatck)*.6)), ipitch2, (p3-iatck)*.6, ipitch2asus	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 512, 2asus2	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, kpitch, "gnawa.pvoc", 1, 7, 2asig2 = (asus+asus2+asus3)*kenvskgoto restrest:	outs	asig1+asig2, asig1+asig2endininstr 4; polvoilength=.17ipitch=p5/161 ;120 is pitch of orig sampleiatck=.0179;assign beginning and end pitches for glissandoipitch=p5/161ipitch2=p9/161;type: where the note's at in the phrase; 0=beg, 1=mid, 2=end, 3=nosus, 4=solotype:if p7=0 kgoto begif p7=1 kgoto midif p7=2 kgoto endif p7=3 kgoto allatckif p7=4 kgoto onenotebeg:katcktm	linseg	0, iatck+.1, iatck+.1kenv	linseg 	0,.001, 10*p4, iatck-.002, 10*p4, .001, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "polvo.pvoc", 1, 1024,1asig1 = atck*kenvtimout iatck+.1, p3-iatck, zero0if katcktm >= iatck kgoto begsusasig2=0kgoto restzero0:asig1=0kgoto begsusbegsus:ksustm	linseg	iatck, p3-iatck, ilength ;fade starts .1 before end???kenvs	linen	15*p4, .01, p3-iatck, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-iatck)-(p8+((p3-iatck)*.6)), ipitch2, (p3-iatck)*.6, ipitch2 asus	pvadd	ksustm, kpitch, "polvo.pvoc", 1, 512, 2asig2 = asus*kenvskgoto restmid:ibeg=iatck*p10idur=iatck-ibegkatcktm	linseg	ibeg, idur+.1, iatck+.1kenv	linseg 	0,.001, 10*p4, idur-.002, 10*p4, .001, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "polvo.pvoc", 1, 1024,1asig1 = atck*kenvtimout idur+.1, p3-(idur+.1), zero1if katcktm >= iatck kgoto midsusasig2=0kgoto restzero1:asig1=0kgoto midsusmidsus:ksustm	linseg	iatck, p3-idur, ilength ;fade starts .1 before end???kenvs	linen	15*p4, .01, p3-idur, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-idur)-(p8+((p3-idur)*.6)), ipitch2, (p3-idur)*.6, ipitch2 asus	pvadd	ksustm, kpitch, "polvo.pvoc", 1, 512, 2asig2 = asus*kenvskgoto restend:ibeg=iatck*p10idur=iatck-ibegkatcktm	linseg	ibeg, idur+.1, iatck+.1kenv	linseg 	0,.001, 10*p4, idur-.002, 10*p4, .001, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "polvo.pvoc", 1, 1024,1asig1 = atck*kenvtimout idur+.1, p3-idur, zero2if katcktm >= iatck kgoto endsusasig2=0kgoto restzero2:asig1=0kgoto endsusendsus:ksustm	linseg	iatck, p3-idur, ilengthkenvs	linen	15*p4, .01, p3-idur, p3*.2kpitch	expseg	ipitch, p8+.01, ipitch, (p3-idur)-(p8+((p3-idur)*.61)), ipitch2, (p3-idur)*.6, ipitch2 asus	pvadd	ksustm, kpitch, "polvo.pvoc", 1, 512, 2asig2 = asus*kenvskgoto restallatck:asig2=0katcktm	linseg	0, p3, iatckkenva	linen	10*p4, .005, p3, .005; rampatck	pvadd	katcktm, 1, "polvo.pvoc", 1, 1024,1asig1 = atck*kenvakgoto restonenote:katcktm	linseg	0, iatck+.1, iatck+.1kenv	linseg 	0,.001, 10*p4, iatck-.002, 10*p4, .001, 0, .1, 0 ; rampatck	pvadd	katcktm, 1, "polvo.pvoc", 1, 1024,1asig1 = atck*kenvtimout iatck+.1, p3-iatck, zero4if katcktm >= iatck kgoto onesusasig2=0kgoto restzero4:asig1=0kgoto onesusonesus:ksustm	linseg	iatck, p3-iatck, ilengthkenvs	linen	15*p4, .01, p3-idur, .01kpitch	expseg	ipitch, p8+.01, ipitch, (p3-iatck)-(p8+((p3-iatck)*.6)), ipitch2, (p3-iatck)*.6, ipitch2asus	pvadd	ksustm, kpitch, "polvo.pvoc", 1, 512, 2asig2 = asus*kenvskgoto restrest:	outs	asig1+asig2, asig1+asig2	endin