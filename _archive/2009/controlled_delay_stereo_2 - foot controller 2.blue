    <instrumentAssignment arrangementId='1' isEnabled='true'>
      <instrument type='blue.orchestra.GenericInstrument'>
        <name>midi switchboard</name>
        <comment></comment>
        <globalOrc></globalOrc>
        <globalSco></globalSco>
        <instrumentText>inotnum notnum
print inotnum

;select channel
if(inotnum == 70) then
gkCurTrack = gkCurTrack + 1
if(gkCurTrack == 5) then 
gkCurTrack = 1
endif
printf &quot;Current Track: %d\\n&quot;, 1, gkCurTrack
gkDirty = 1
endif


;expr mode - 0; control loop length and regen, else control in/out volume
if(inotnum == 63) then
gkCurMode = gkCurMode + 1
if(gkCurMode == 2) then 
gkCurMode = 0
endif
printf &quot;Current Mode: %d\\n&quot;, 1, gkCurMode
endif

;
; Toggle Volume In
;
if(inotnum == 61) then
if(gkCurTrack == 1) then
if(gkloop1 == 1) then
FLsetVal	 	1, 0, gihbut1
else
FLsetVal	 	1, 1, gihbut1
endif 
endif

if(gkCurTrack == 2) then
if(gkloop2 == 1) then
FLsetVal	 	1, 0, gihbut2
else
FLsetVal	 	1, 1, gihbut2
endif 
endif

if(gkCurTrack == 3) then
if(gkloop3 == 1) then
FLsetVal	 	1, 0, gihbut3
else
FLsetVal	 	1, 1, gihbut3
endif 
endif

if(gkCurTrack == 4) then
if(gkloop4 == 1) then
FLsetVal	 	1, 0, gihbut4
else
FLsetVal	 	1, 1, gihbut4
endif 
endif

endif

;
; Toggle Volume Out
;
if(inotnum == 62) then

if(gkCurTrack == 1) then
if(gkloopread1 == 1) then
FLsetVal	 	1, 0, gihbutread1
else
FLsetVal	 	1, 1, gihbutread1
endif 
endif

if(gkCurTrack == 2) then
if(gkloopread2 == 1) then
FLsetVal	 	1, 0, gihbutread2
else
FLsetVal	 	1, 1, gihbutread2
endif 
endif

if(gkCurTrack == 3) then
if(gkloopread3 == 1) then
FLsetVal	 	1, 0, gihbutread3
else
FLsetVal	 	1, 1, gihbutread3
endif 
endif

if(gkCurTrack == 4) then
if(gkloopread4 == 1) then
FLsetVal	 	1, 0, gihbutread4
else
FLsetVal	 	1, 1, gihbutread4
endif 
endif

endif


;ih_update_tap_1
;
; Tap Tempo
;
if(inotnum == 65) then

if(gkCurTrack == 1) then
if(gk_update_tap_1 == 1) then
FLsetVal	 	1, 0, gih_update_tap_1
else
FLsetVal	 	1, 1, gih_update_tap_1
endif 
endif

if(gkCurTrack == 2) then
if(gk_update_tap_2 == 1) then
FLsetVal	 	1, 0, gih_update_tap_2
else
FLsetVal	 	1, 1, gih_update_tap_2
endif 
endif

if(gkCurTrack == 3) then
if(gk_update_tap_3 == 1) then
FLsetVal	 	1, 0, gih_update_tap_3
else
FLsetVal	 	1, 1, gih_update_tap_3
endif 
endif

if(gkCurTrack == 4) then
if(gk_update_tap_4 == 1) then
FLsetVal	 	1, 0, gih_update_tap_4
else
FLsetVal	 	1, 1, gih_update_tap_4
endif 
endif

endif

end:
turnoff</instrumentText>
