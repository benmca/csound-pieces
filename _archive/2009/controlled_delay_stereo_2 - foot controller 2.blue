    <instrumentAssignment arrangementId='2' isEnabled='true'>
      <instrument type='blue.orchestra.GenericInstrument'>
        <name>untitled</name>
        <comment></comment>
        <globalOrc></globalOrc>
        <globalSco></globalSco>
        <instrumentText>kExpressionA chanctrl 2, 27
kExpressionB chanctrl 1, 7
kExpressionA = kExpressionA* 0.00787401574
kExpressionB = kExpressionB* 0.00787401574

;
; clear dirty bit and skip param initialization
;	dirty bit is set when channel is changed.
;
if(gkDirty == 1) then
gkDirty = 0
kgoto end
endif


;
; gkCurMode = 0: set loop/regen
;
if(gkCurMode == 0) then

if(gkCurTrack == 1) then
if(kExpressionA*gkmaxdel == gkrate1) kgoto checkb1
FLsetVal	 	1, kExpressionA*gkmaxdel, gihtap1
checkb1:
if(kExpressionB == gkregen1) kgoto end
FLsetVal	 	1, kExpressionB, gihregknob1
endif

if(gkCurTrack == 2) then
if(kExpressionA*gkmaxdel == gkrate2) kgoto checkb2
FLsetVal	 	1, kExpressionA*gkmaxdel, gihtap2
checkb2:
if(kExpressionB == gkregen2) kgoto end
FLsetVal	 	1, kExpressionB, gihregknob2
endif

if(gkCurTrack == 3) then
if(kExpressionA*gkmaxdel == gkrate3) kgoto checkb3
FLsetVal	 	1, kExpressionA*gkmaxdel, gihtap3
checkb3:
if(kExpressionB == gkregen3) kgoto end
FLsetVal	 	1, kExpressionB, gihregknob3
endif

if(gkCurTrack == 4) then
if(kExpressionA*gkmaxdel == gkrate4) kgoto checkb4
FLsetVal	 	1, kExpressionA*gkmaxdel, gihtap4
checkb4:
if(kExpressionB == gkregen4) kgoto end
FLsetVal	 	1, kExpressionB, gihregknob4
endif

endif

if(gkCurMode == 1) then

if(gkCurTrack == 1) then
FLsetVal	 	1, kExpressionA, gihinput1
FLsetVal	 	1, kExpressionB, gihoutput1
endif

if(gkCurTrack == 2) then
FLsetVal	 	1, kExpressionA, gihinput2
FLsetVal	 	1, kExpressionB, gihoutput2
endif

if(gkCurTrack == 3) then
FLsetVal	 	1, kExpressionA, gihinput3
FLsetVal	 	1, kExpressionB, gihoutput3
endif

if(gkCurTrack == 4) then
FLsetVal	 	1, kExpressionA, gihinput4
FLsetVal	 	1, kExpressionB, gihoutput4
endif

endif

end:</instrumentText>
