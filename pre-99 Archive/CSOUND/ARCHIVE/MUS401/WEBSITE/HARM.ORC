sr=44100kr=2205ksmps=20nchnls=1instr 1 ; A  pitch shifteridelmax = .1ipshift = p4 ; in semitonesifracoct = ipshift/12ibase = cpsoct(8.00)inewfrq = cpsoct(8+ifracoct)iphsfreq = ((inewfrq/ibase) - 1)/ idelmax;change the soundfile to one of your own!a1  soundin "LittleWingBeg.aiff", 0aphs1   phasor  -iphsfreqatbl1   tablei  aphs1,  1, 1, 0, 1aphs2   phasor -iphsfreq,.5atbl2   tablei aphs2, 1, 1, 0, 1ax    delayr idelmaxadel1 deltapi aphs1 * idelmaxadel2 deltapi aphs2 * idelmax      delayw  a1asig1 = (adel1*atbl1) * p5asig2 = (adel2*atbl2) * p5out asig1+asig2endin;f1 0 16384 9 .5 1 0;i1 0 10 -6 .4;i1 0 10 6 .7;f 0 2;f 0 4;f 0 6;f 0 8;e