sr=22050kr=2205ksmps=10nchnls=1ga1 init 0instr 8 ; Csound realization a Frequency Modulation Trumpetiamp = p4 * 25000 ;Amplitude of the first carrier.iamp2 = iamp * p11 ;.2 ;Amplitude of the second carrier.ifundfreq = p5 ;The fundemental frequency.iformant = int((p10/ifundfreq + .5)) * ifundfreqifm=ifundfreq*p12idev1   = ifm * p7 * p6idev2   = p8 * p6; VIBRATOarand   randi   .007, 15kvibamp oscil1  0, .007, p3, 2                ;f2 0 4097 9 .5 1 0 (Half a period of a sine wave)avibosc oscil   kvibamp, 6.5, 1aporta  linseg  0, .06, .03, p3-.06, .02avib    =       (1+arand) * (1+avibosc) * (1+aporta)aenv1   envlpx  1, p9, p3, .15, 3, .9, .01aenv2   envlpx  1, .1, p3, .01, 3, .9, .01aenv3   envlpx  1, p9, p3, .15, 3, .9, .01;MODULATORamod1    oscili aenv2*idev1, ifm*avib, 1amod2 = amod1*idev2;CARRIER1acar1   oscili aenv1*iamp, (ifundfreq*avib)+amod1, 1;CARRIER2acar2   oscili iamp2*aenv3, (iformant*avib) + amod2, 1asig = acar1 + acar2ga1=(ga1+asig) * .02out asigendininstr 99a1 reverb       ga1, p4ga1 = 0out a1endin