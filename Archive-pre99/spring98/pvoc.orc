sr=22050kr=2205ksmps=10nchnls=1instr 1ipitch = p5/289aenv	linen	100*p4, .01, p3, .01ktm	line	0, 1.01, 1.01asig	pvadd	ktm, 1, "miro.pvoc", 1, 100, 1		out	aenv*asigendininstr 2;; miroslav upright sample (harmonic); ipitch=p5/289iatck=.06katcktm	linseg	0, iatck+.5, iatck+.5; ampskenva	linseg	10*p4, iatck, 10*p4, .01, 0, .5, 0; rampkenvb	linseg 	0,.01, 1, iatck-.02, 1, .01, 0, .5, 0atck	pvadd	katcktm, 1, "miro.pvoc", 1, 1024,1asig1 = kenvb*(atck*kenva);;prog control;timout iatck+.1, p3-(iatck+.1), susif katcktm > iatck kgoto susasig2=0kgoto restsus:asig1=0ksustm	linseg	iatck, p3-iatck, .3;kenvs	linen	15*p4, .01, p3-iatck, .01kenvs	linseg	0, .01, 15*p4, p3-iatck, 15*p4kenvsb	linen	1, .01, .1+(p3-iatck), .01asus	pvadd	ksustm, ipitch, "miro.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, ipitch, "miro.pvoc", 1, 40, 2asus3	pvadd	ksustm, ipitch, "miro.pvoc", 1, 7, 2asig2 = kenvsb*((asus+asus2+asus3)*kenvs)rest:	out	asig1+asig2	endininstr 3ipitch=p5/120iatck=.08katcktm	linseg	0, iatck+.5, iatck+.5; ampskenva	linseg	10*p4, iatck, 10*p4, .01, 0, .5, 0; rampkenvb	linseg 	0,.01, 1, iatck-.02, 1, .01, 0, .5, 0atck	pvadd	katcktm, 1, "gnawa.pvoc", 1, 1024,1asig1 = kenvb*(atck*kenva);;prog control;timout iatck+.1, p3-(iatck+.1), susif katcktm > iatck kgoto susasig2=0kgoto restsus:asig1=0ksustm	linseg	iatck, p3-iatck, .3;kenvs	linen	15*p4, .01, p3-iatck, .01kenvs	linseg	0, .01, 15*p4, p3-iatck, 15*p4kenvsb	linen	1, .01, .1+(p3-iatck), .01asus	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 512, 2, 4asus2	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 40, 2asus3	pvadd	ksustm, ipitch, "gnawa.pvoc", 1, 7, 2asig2 = kenvsb*((asus+asus2+asus3)*kenvs)rest:	out	asig1+asig2endin