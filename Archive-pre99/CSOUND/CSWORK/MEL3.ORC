sr=44100kr=4410ksmps=10nchnls=2ga1 init	0ga2	init	0;;;instr 1iamp=p4idur=p3iamp=p4ifc = p5ipan = p6		; *idist = p7		; * locsig argsipct = p8		; *iratio1 = p9index1 = p10iratio2 = p11index2 = p12iratio3 = p13index3 = p14krmp	linen	iamp, .01, idur, .01klin	expseg	10, p3-3, 1, 3, 1klin2	expseg	2, p3-3, 1, 3, 1amodfreq1 = ifc * iratio1kdev1 = amodfreq1 * (index1)amodfreq2 = (klin*iratio2)*amodfreq1kdev2 = amodfreq2 * (index2)amodfreq3 = (iratio3)*amodfreq2kdev3 = amodfreq3 * (index3)amod3   oscili	kdev3, amodfreq3, 1amod2   oscili	kdev2, amodfreq2+amod3, 1amod1	oscili	kdev1, amodfreq1+amod2, 1acar1	oscili	krmp, ifc+amod1, 1asnd = acar1; locsig statementsar, al		locsig		asnd, ipan, idist, ipctar1, ar2	locsend			outs	ar, al; output to reverbga1=ga1+ar1ga2=ga2+ar2endin;;;	2;;;instr 2iamp=p4idur=p3iamp=p4ipitch = p5ipan = p6		; *idist = p7		; * locsig argsipct = p8		; *iratio = 2imodfreq = p5 * iratioindex = 1idev = imodfreq * indexkrmp	linen	iamp, .01, idur, .01amod	oscili	idev, imodfreq, 1acar1	oscili	krmp, p5+amod, 1;acar2	oscili	krmp*.7, 2*(p5+amod), 1;acar3	oscili	krmp*.4, 4*(p5+amod), 1klin	line	1, p3, p5*7acar1	tone	acar1, klinasnd = acar1; locsig statementsar, al		locsig		asnd, ipan, idist, ipctar1, ar2	locsend			outs	ar, al; output to reverbga1=ga1+ar1ga2=ga2+ar2endininstr 99a1	reverb2	ga1, p4, .5a2	reverb2	ga2, p4, .5	outs	a1,a2ga1=0ga2=0endin