sr = 44100kr=4410ksmps = 10 nchnls = 1instr 1    insnd = 10    ibasfrq = 44100 / ftlen(insnd)   ; Use original sample rate of insnd file    kamp   expseg 10000, p3/2, 16000, p3/2, 10000    ;kpitch line ibasfrq, p3, ibasfrq    kdens  linseg 10, p3*.3, 10, p3*.01, 5, p3*.3, 5, p3*.01, 100, p3*.38, 100    kaoff  line 100, p3, 1000    kpoff  line .01, p3, .001    kgdur  line 13, p3, 13    imaxgdur =  13  ar  grain kamp, ibasfrq, kdens, kaoff, kpoff, kgdur, insnd, 5, imaxgdur, 1	;igrnd = 1 : turns off  																				;grain offset randomness  																				 out arendin