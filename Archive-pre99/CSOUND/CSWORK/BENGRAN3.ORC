sr = 44100kr=4410ksmps = 10 nchnls = 2ga1 init	0ga2	init	0instr 1idur = p3iampoff = p4ipitchoff = p5ipan = p6idist = p7ipct = p8idens = p9igdur = p10ipscale = p11insnd = 10ibasfrq = (44100 / ftlen(insnd)) * ipscale   ; Use original sample rate of insnd fileimaxgdur =  .954aenv		linen	1, .01, p3, .01asnd		grain	1000, ibasfrq, idens, iampoff, ipitchoff, igdur, insnd, 5, imaxgdur, 0.0ar, al		locsig	aenv*asnd, ipan, idist, ipctar1, ar2	locsend			outs	ar, al; output to reverbga1=ga1+ar1ga2=ga2+ar2 outs ar , alendininstr 99a1	reverb2	ga1, p4, .5a2	reverb2	ga2, p4, .5	outs	a1,a2ga1=0ga2=0endin