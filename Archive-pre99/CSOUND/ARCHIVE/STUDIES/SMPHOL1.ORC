sr=44100kr=4410ksmps=10nchnls=2instr 1  asrc   buzz      10000,440,20, 1     ; band-limited pulse train  adif   diff      asrc                ; emphasize the highs  anew   balance   adif, asrc          ;   but retain the power  agate  reson     asrc,0,440          ; use a lowpass of the original  asamp  samphold  anew, agate         ;   to gate the new audiosig  aout   tone      asamp,100 outs asamp, asampendin