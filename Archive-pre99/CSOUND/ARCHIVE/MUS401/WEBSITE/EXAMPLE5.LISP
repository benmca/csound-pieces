(in-package :stella); CommonMusic Score for simple FM instrument(defobject fm-simp (csound-note)   ((instr :initform "i7") dur amp ampfun carrier modratio modindex)   (:parameters instr time dur amp ampfun carrier modratio modindex))(defun fmexamp (start-time)  (algorithm nil fm-simp (start start-time length 22)    (setf rhythm 1)    (if (or (= count 10) (= count 21))        (progn           (setf rhythm 10)           (setf dur 10))        (progn           (setf rhythm 1)           (setf dur .8)))    (setf amp .5)    (setf ampfun (item (items (items 2 for 5) (items 3 for 6))))    (setf carrier (pitch 'c4))    (setf modratio (item (items (items 1 for 11) (items 1.413 for 11))))    (setf modindex (interpl count 0 1.0 10 7.0 11 1.0 21 7.0))))(defun make-score (&key (start-time 1.) (my-scorefile "fmexamp.sco"))  (let* ((myheader (header "; Notelist for Csound Simple FM Example"                         "f 1 0 16384 10 1"                         "f 2 0 16385 9 .5 1 0"                         "f 3 0 16385 5 1 16384 .0001")))    (fheader  my-scorefile myheader)    (merge all ()        (fmexamp start-time))    (write-cs "all" :filename my-scorefile)))