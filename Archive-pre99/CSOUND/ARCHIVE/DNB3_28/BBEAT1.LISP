(in-package :stella) (defparameter *totaldur* 0) ; (defobject quick (csound-note)   ((instr :initform "i1")  dur amp)   (:parameters instr  time dur amp))  (defun opening (st len)  (algorithm nil quick (start st length len)	(setf rhythm (item                       (items                        (rhythms w tempo (tempo 84))                        (rhythms w. tempo (tempo 84))                           ))) 	(setf dur rhythm) 	(setf amp 1) ))   (defun ms (&key (start-time 1.)(my-scorefile "Apps:Csound:Moore:opening1.sco"))   (let* ((myheader (header "" 			   )))     (fheader my-scorefile myheader)     (merge all () 		(opening 1 3)             )     (open-cmd my-scorefile) 	(mix-cmd "all")))