sr=22050kr=2205ksmps=10nchnls=1instr 1iamp = p4 * 25000ifundfreq = p5 idev = ifundfreq*p6amod	oscili	idev, ifundfreq, 1acar	oscili	iamp, amod+ifundfreq, 1out acarendininstr 8iamp = p4 * 25000 ;Amplitude of the first carrier.iamp2 = iamp * p5 ;.2 ;Amplitude of the second carrier.ifundfreq = p6 ;The fundemental frequency.iformant = int((p7/ifundfreq + .5)) * ifundfreqifm=ifundfreqidev1   = p8 * ifmasca	linseg	0, .1*p3, 1, .9*p3,1, .99*p3, 0;MODULATORamod1    oscili idev1, ifm, 1;CARRIER1acar1   oscili iamp, (ifundfreq)+amod1, 1;CARRIER2acar2   oscili iamp2, (iformant) +amod1, 1asig = (acar1 + acar2)*ascaout asigendin