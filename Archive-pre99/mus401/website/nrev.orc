; A REVERB INSTRUMENT TRANLATED FROM THE CCRMA "NREV"instr 99 ;accepts globals as input and adds reverbirevfactor =  p4 ; effects reverb timeilowpass = sr * .4ioutputscale =  p5;THE VALUES BELOW ARE GOOD ONLY FOR SR=22050idel1 = 1237.000/sridel2 = 1381.000/sridel3 = 1607.000/sridel4 = 1777.000/sridel5 = 1949.000/sridel6 = 2063.000/sridel7 = 307.000/sridel8 = 97.000/sridel9 = 61.000/sridel10 = 53.000/sridel11 = 37.000/sridel12 = 31.000/sricsc1 = .822 * irevfactoricsc2 = .802 * irevfactoricsc3 = .773 * irevfactoricsc4 = .753 * irevfactoricsc5 = .753 * irevfactoricsc6 = .753 * irevfactoricsc7 = .7 * irevfactor;LEFT CHANNELacomb1  comb    ga1, icsc1, idel1acomb2  comb    ga1, icsc2, idel2acomb3  comb    ga1, icsc3, idel3acomb4  comb    ga1, icsc4, idel4acomb5  comb    ga1, icsc5, idel5acomb6  comb    ga1, icsc6, idel6acomball = acomb1 + acomb2 + acomb3 + acomb4 + acomb5 + acomb6allp1   alpass  acomball, icsc7, idel7allp2   alpass  allp1, icsc7, idel8allp3   alpass  allp2, icsc7, idel9alow    tone    allp3, ilowpassallp4   alpass  alow, icsc7, idel10allp5   alpass  allp4, icsc7, idel11arevout1 =  allp5 * ioutputscale;RIGHT CHANNELacomb1  comb    ga2, icsc1, idel1acomb2  comb    ga2, icsc2, idel2acomb3  comb    ga2, icsc3, idel3acomb4  comb    ga2, icsc4, idel4acomb5  comb    ga2, icsc5, idel5acomb6  comb    ga2, icsc6, idel6acomball = acomb1 + acomb2 + acomb3 + acomb4 + acomb5 + acomb6allp1   alpass  acomball, icsc7, idel7allp2   alpass  allp1, icsc7, idel8allp3   alpass  allp2, icsc7, idel9alow    tone    allp3, ilowpassallp4   alpass  alow, icsc7, idel10allp6   alpass  allp4,icsc7, idel12arevout2 =  allp6 * ioutputscale;REVERB OUTPUT WITH LEFT CHANNEL DELAYED BY .05 SECONDS;arevout1x delay arevout1, .05;outs arevout1x, arevout2;REVERB OUTPUT WITH LEFT AND RIGHT CHANNELS MIXED;outs (arevout1*.75)+(arevout2*.25), (arevout1*.25)+(arevout2*.75);REVERB OUTPUT (NORMAL)outs arevout1, arevout2ga1 = 0ga2 = 0endin