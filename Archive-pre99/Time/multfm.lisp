(in-package :stella)(defun write-cs (container &key (filename "drum2.sco"))	(open-cmd filename)	(mix-cmd container))(defparameter ratnum 0)(defparameter ratden 0)(defobject form (csound-note)        ((instr :initform "i1") dur amp fc fm index)        (:parameters instr time dur amp fc fm index))(defobject noform (csound-note)        ((instr :initform "i2") dur amp freq index)        (:parameters instr time dur amp freq index))(defun test (mystart len)  (algorithm nil form (start mystart length len)    (setf rhythm (item (rhythms h tempo 40 in heap)))    (setf rhythm (* rhythm (between .99 1.01)))    (setf ratnum 6)    (setf ratden 1)    (setf fc (* 22.5 ratden))    (setf fm (* 22.5 ratnum))        (setf index 2)    (setf dur rhythm)    (setf amp 5000)))(defun ms (&key (start-time 1.)                        (filename "Retard:Audio:Csound:multfm.sco"))  (let* ((header (header "f 1 0 16384 10 1")))    (fheader filename header)    (merge all (start start-time)      (test 1 10)      )    (write-cs "all" :filename filename)))