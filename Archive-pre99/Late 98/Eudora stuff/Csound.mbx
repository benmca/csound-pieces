 -.0420264804608, .0167444101633, .00396788361296,-.00128920335614, 
 -.000509505399

;coifman 6
ifn  ftgen     5, 0, 8, -2, (sqrt(15)-3.0)/32.0, (1.0-sqrt(15))/32.0, 
(3.0-sqrt(15))/16.0, (sqrt(15)+3.0)/16.0, (sqrt(15)+13.0)/32.0, 
(9.0-sqrt(15))/32.0

;mirror filter
ifn  ftgen     6, 0, 16, -7, 0, 16, 0

;size table
ifn  ftgen     7, 0, 8, -2, 4, 10, 12, 12, 6

gifilt    =    0
gimirr    =    0
gifsz     =    0

     zakinit   1, 1022

;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
     instr     1
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
; MirrorFilter
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
ifilter   =    p4
imirror   =    6
isztab    =    7
ifiltsz   table     ifilter-1, isztab
indx =    0
loop:
ifil table     ifiltsz-indx-1, ifilter
     itablew   (frac((indx+1)/2)==0 ? +1:-1)*ifil, indx, imirror
indx =    indx+1
     if (indx<ifiltsz) igoto loop
gifilt    =    ifilter
gimirr    =    imirror
gifsz     =    ifiltsz
     endin


;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
     instr     3
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
; Toneburst
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
ivectsz   =    p4    ; audio table size
iwavlen   =    p5    ; length of wavelet
i1pos     =    p6    ; position of the single '1'
istart    =    0     ; start of storage area

ipass     =    0
iovec     =    istart
ivlen     =    ivectsz
main:
iivec     =    iovec
iovec     =    iivec+ivlen
ivlen     =    ivlen/2
ipass     =    ipass+1
     if (ivlen>iwavlen) igoto main
ii   =    0
loop1:
     ziw  (ii==i1pos ? 1:0), iovec+ii
ii   =    ii+1
     if (ii<ivlen) igoto loop1
givec     =    iovec
gilen     =    ivlen
giseq     =    ipass+1
     endin

;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
     instr     4
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
; AntiConvolve
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
ivector   =    p4
ivectsz   =    p5
iseqtab   =    p6
ifilter   =    gifilt; filter table number
imirror   =    gimirr; mirror table number
ifiltsz   =    gifsz ; filter table size
istart    =    0

iovec     =    givec
ivlen     =    gilen
ipass     =    giseq-1
main:
istp table     ipass, iseqtab
ipass     =    ipass-1
ifilter   =    (istp==0 ? ifilter:imirror)
iivec     =    iovec
iovec     =    iivec-ivlen*2
ii   =    0
loop0:
     ziw  0, iovec+ii
ii   =    ii+1
     if (ii<ivlen*2) igoto loop0
ij   =    0
loop1:
ii   =    0
loop2:         ;OutVector[(2*j+i)%(2*InVecLen)]+=Filter[i]*InVector[j]
ipos =    2*ij+ii-int((2*ij+ii)/(2*ivlen))*2*ivlen
i_f  table     ii, ifilter
i_v  zir  iivec+ij
i_vv zir  iovec+ipos
i_vv =    i_vv+i_v*i_f
     ziw  i_vv, iovec+ipos
ii   =    ii+1
     if (ii<ifiltsz) igoto loop2
ij   =    ij+1
     if (ij<ivlen) igoto loop1
ivlen     =    ivlen*2
     if (ivlen<ivectsz) igoto main
indx =    0
imin =    32767
imax =    -32768
load:
isig zir  istart+indx
     if (isig<=imax) igoto next1     ; find maximum and minimum
imax =    isig                    ; to perform normalization
     igoto     next2
next1:    if (isig>=imin) igoto next2
imin =    isig
next2:    itablew   isig, indx, ivector
indx =    indx+1
     if (indx<ivectsz) igoto load
indx =    0
norm:
isig table     indx, ivector
isig =    (isig-imin)/(imax-imin)-.5 ; scale the table to the
     itablew   isig, indx, ivector        ; range [-.5, +.5]
indx =    indx+1
     if (indx<ivectsz) igoto norm

     endin

;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
     instr     5
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
; Test : A simple oscilator
;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
ivector   =    p4
iamp =    p5
ifrq0     =    p6
ifrq1     =    p7

kamp linseg    0, p3/8 , iamp, 3*p3/4, iamp, p3/8, 0
kfrq linseg    ifrq0, p3, ifrq1
asig oscil     kamp, kfrq, ivector

     out  asig

     endin

;Start of WPT.SCO

;signal table
f8 0 512 -7 0 512 0

;filter sequence table
;             512 256 128 64 32 16 8
f9 0 16 -2     0   0   0   0  0  0 0 ; try any seq of 0s and 1s

;initialize filter
;         filter (1-5)
i1 0 0       1                       ; try filters 1 to 5

;* * ToneBurst * *
;  str dur  vecsz wavsz 1pos         ; try any 2<=wavsz<=512 (must be 
pow'of-2)
i3  0   0    512    8     0          ; try 1pos from 0 to wavsz-1

;* * AntiConvolve * *
;  str dur  vectab vecsz seqtab
i4  0   0     8     512    9

;=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=;
; Lets hear it:
;  str dur vectab dynrng  frq0 frq1
i5  0  5     8    30000    1    55   ;you gotta find'em in freq
e

