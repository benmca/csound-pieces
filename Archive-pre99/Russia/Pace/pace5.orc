sr=44010kr=4401ksmps=10nchnls=2instr 1idur=p3iamp=p4*10ipitch=p5ipartial=p6inum=int(p6)+1;simple envelope for attack;decent cheap polish texture if many notes are present around one pitchkenv	linen		iamp, idur*.2, idur, idur*.5avib	oscili	10, 2, 1asig	buzz		kenv, ipitch, inum, 1amod	oscili	ipartial*(ipitch*2), ipitch*2, 1acar	oscili	kenv*.5, ipitch+amod, 1asig2 oscili	kenv*1.1, ipitch+avib, 1outs asig+asig2+acar, asig+asig2+acarendininstr 2idur=p3iamp=p4ipitch=p5ictrindx=p6kenv	oscil1i	0, iamp, idur, 2;kenv	linen	iamp, .01, p3, .01knois	randi		1, 30kenv2	linseg	1.5, p3, 1; insert kenv2 in pitch of carrier=gradual slide to pitch if 1->1.01kpit	oscil1i	0, 1,idur*2, 1amod	oscili	(ictrindx+cpsoct(knois))*(ipitch*2), ipitch*2, 1acar	oscili	kenv, (kpit*(ipitch+amod)), 1; in kpit, 	if idur>1, slower half sine shape do sound,;		if <1, quicker...	outs		acar, acarendininstr 3idur=p3iamp=p4ipitch=p5imax=3irat=3kenv	oscil1i	0, iamp, idur, 3	;attack env #3;kenv2	oscil1i	0, iamp, idur, 2	;attack env;kenv	linen	iamp, .01, p3, .01	;ramp envaosc1	oscili	iamp, 1/idur, 2aosc2	oscili	(ipitch*irat)*imax, 1/idur, 2aosc3 oscili	aosc2, ipitch*irat, 1aosc4	oscili	aosc1, ipitch+aosc3, 1amod1	oscili	2*(ipitch*2), ipitch*4, 1a1	oscili	kenv*.5, ipitch*2+amod1, 1amod2	oscili	3*(ipitch*3), ipitch*4, 1a2	oscili	kenv*.4, ipitch*3+amod2, 1amod3	oscili	4*(ipitch*3), ipitch*5, 1a3	oscili	kenv*.3, ipitch*4+amod3, 1aosc=aosc4+a1+a2+a3outs aosc, aosc;amod	oscili	4*(ipitch*2), ipitch*2, 1;acar	oscili	kenv, ipitch+amod, 1; in kpit, 	if idur>1, slower half sine shape do sound,;		if <1, quicker...;	outs	acar, acarendin