sr=22050kr=2205ksmps=10nchnls=2ga1 	init	0ga2	init	0	instr	1;;	High Waveshaping Stuff (Scene 1);idur=p3iamp=p4ifreq=p5ipan=p6idist=p7ipct=p8;;	Distortion indx and amp scalar;k1	linen		iamp*.5, .01, idur, .01k2	linseg	.1, p3*.2, .05, p3*.4, .25, p3*.4, .001	; freq of change in dist indexk11	phasor	k2k2	tablei	k11, 3, 1, 0, 1k2	=	(k2/5)+.1; ; +.1 in k2 gives minimum of .1 to dist indxkamp	=	k1*k2a1	oscili	kamp, ifreq, 1asig2	tablei	a1+k2, 2;;	Why Tablei instead of Oscili?;a1, a2	locsig	asig2*kamp, ipan, idist, ipctar1, ar2	locsendga1=ga1+ar1ga2=ga2+ar2	outs	a1, a2endin	instr	2;;	Low Drone (Scene 1);idur=p3iamp=p4ifreq=p5ipan=p6idist=p7ipct=p8klin	linen	1, .1, idur, .05;grain stuffk1	linseg	50, idur, 400	; xdensk2	linseg	.05, idur, .005	; kgdura1	grain		iamp*.4, ifreq*2, k1, 0, 10, k2, 2, 4, 1a2	grain		iamp*.4, ifreq*3, k1, 0, 10, k2, 2, 4, 1a1=(a1+a2)*.5; fm drone with random indexka1	linen		iamp*.5, .01, idur, .01anois	randh		4, .5anois	tone	anois, 2ifreq1=ifreq/2ka2	linseg		0, idur, .3;amod	oscili	(cpsoct(knois))*(ifreq1*2), ifreq1*2, 2;acar	oscili	ka1*.3, (ifreq1+amod), 2acar,acmp	sndwarp	ka1, ka2, .25*cpsoct(anois), 5, 0, 20, 10, 10, 3, 1acar	balance	acar, acmp;four oscillators with different vibratoavib	oscili	.007, 1, 2avib2	oscili	.007, 6, 2avib3	oscili	.007, .25, 2av1	oscili	iamp*.9, (ifreq),2;*(1+avib), 2av2	oscili	iamp*.85, (ifreq)*(1+avib2), 2av3	oscili	iamp*.85, (ifreq)*(1+avib3), 2;;all together now;a1=(av1+av2+av3+a1+acar)*klina1, a2		locsig	a1, ipan, idist, ipctar1, ar2	locsendga1=ga1+ar1ga2=ga2+ar2	outs	a1, a2endininstr 99a1	reverb2	ga1, 15, .5a2	reverb2	ga2, 15, .5	outs	a1,a2ga1=0ga2=0endin